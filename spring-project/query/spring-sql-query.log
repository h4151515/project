-- database delete(initialize)
DROP ALL OBJECTS;

-- create table query
CREATE TABLE MEMBER
(
	id bigint GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
	userid VARCHAR(255),
	name VARCHAR(255)
);


